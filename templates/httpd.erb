NameVirtualHost <%= @ipaddress %>:80

<VirtualHost <%= @ipaddress %>:80 >
ServerAdmin <%= email %>
ServerName <%= @fqdn %>

ProxyRequests Off

<Proxy *>
    Order allow,deny
    Allow from all
</Proxy>

ProxyPass / http://localhost:<%= port %>/
ProxyPassReverse / http://localhost:<%= port %>/
</VirtualHost>
